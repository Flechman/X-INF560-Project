set term postscript eps color 8
set ytic auto

unset label

set autoscale y
set boxwidth 0.5
set style fill solid

set ylabel "Latency (s)"
set xlabel "Images"
set xtics rotate by 45 right
set title title_text

set output output_file
plot input_file using 1:3:xtic(4) with boxes notitle,\
         input_file using 1:($3+0.02):3 with labels notitle;
set output
quit
